@viewModel SnowFur.ViewModels.Controls.ReservationForm, Snowfur

<h3>Rezervácia izby</h3>
<form class="form">
    <dot:Repeater DataSource="{value: Orders}">
        <div class="form-group row">
            <label class="col-md-4 col-sm-8">{{value: Name}} - ({{value: Price}} EUR) </label>
            <dot:CheckBox class="col-md-1" Checked="{value: IsOrdered}" />
        </div>
    </dot:Repeater>
</form>

