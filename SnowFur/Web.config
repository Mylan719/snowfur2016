  <connectionStrings>
    <add name="ApplicationContext" providerName="System.Data.SqlClient" connectionString="Data Source=.\SQLEXPRESS; Initial Catalog=SnowFur; Integrated Security=True" />
  </connectionStrings>
  <appSettings>
    <add key="WebsiteDomain" value="http://www.snowfur.info/" />
    <add key="AdminEmail" value="info@snowfur.info" />
  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="SpecifiedPickupDirectory" from="info@snowfur.info">
        <specifiedPickupDirectory pickupDirectoryLocation="C:\temp\mailpickup" />
      </smtp>
    </mailSettings>
  </system.net>
  <!--
